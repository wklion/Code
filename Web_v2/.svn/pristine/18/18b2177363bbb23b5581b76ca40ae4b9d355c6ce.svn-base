.alarmProductListWindow {
  position: absolute;
  height: 80%;
  width: 60%;
  left: 50%;
  top: 40px;
  transform: translateX(-50%);
  color: white;
  background-color: #165983;
}

.alarmProductListWindow .thisTitle {
  height: 4%;
  line-height: 26px;
  padding: 3px;
  text-align: center;
  font-weight: bold;
  font-size: 18px;
}

.alarmProductListWindow .close {
  color: red;
  width: 30px;
  height: 30px;
  font-size: 30px;
}

.alarmProductListWindow iframe {
  width: 98%;
  height: 100%;
  margin: 0px auto;
  display: block;
}

.alarmProductListWindow .productControl {
  height: 5%;
  margin-bottom: 0.5%;
  text-align: center;
}

.alarmProductListWindow .productControl button {
  height: 100%;
  width: 80px;
  background-color: transparent;
  margin-right: 0.5%;
}

.alarmProductListWindow .productShow {
  width: 100%;
  height: 90%;
  margin: 0 auto;
}
