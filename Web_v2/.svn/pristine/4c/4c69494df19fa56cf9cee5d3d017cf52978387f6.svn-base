*,
*:before,
*:after {
  box-sizing: border-box; }

/**
 * ## Position & Box
 */
.absolute {
  position: absolute; }

.w, .f, html,
body, .full, .body, .body .banner,
.body .content {
  position: relative;
  width: 100%; }

.h, .f, html,
body, .full, .body {
  position: relative;
  height: 100%; }

.full {
  margin: 0;
  padding: 0; }

.auto-w {
  width: auto !important; }

.auto-h {
  width: auto !important; }

/**
 * ## align & float
 */
.align-center {
  text-align: center; }

.align-left {
  text-align: left; }

.align-right {
  text-align: right; }

.left {
  float: left; }

.right {
  float: right; }

.invisible {
  visibility: hidden; }

.hide {
  display: none; }

.hidden, .user-wrapper:not(.logged) .user, .user-wrapper:not(.logged) .logout, .user-wrapper.logged .login {
  display: none !important; }

.hoverable {
  transition: box-shadow .25s;
  box-shadow: 0; }

.hoverable:hover {
  transition: box-shadow .25s;
  box-shadow: 0 8px 17px 0 rgba(0, 0, 0, 0.2), 0 6px 20px 0 rgba(0, 0, 0, 0.19); }

/**
 * banner
 */
.banner {
  color: #ced4da;
  background-color: #1c272d;
  padding: 0 1rem;
  display: flex;
  flex-wrap: nowrap;
  align-items: center; }
  .banner a {
    color: inherit;
    text-decoration: none; }
  .banner .brand {
    display: flex;
    flex-wrap: nowrap;
    align-items: center;
    justify-content: center;
    flex: 0 0 auto; }
    .banner .brand .logo {
      height: 51px;
      width: 485px;
      background-size: cover;
      background-image: url("../images/logo.png"); }
    .banner .brand .title {
      padding-left: 1rem;
      font-size: 2em;
      flex-shrink: 0; }
      .banner .brand .title:hover, .banner .brand .title:focus {
        color: #fff; }
  .banner .navbar {
    margin: 0;
    padding: 0 1rem;
    border-radius: 0;
    width: calc(100% - 585px); }
  .banner .user-wrapper {
    display: flex;
    flex-wrap: nowrap;
    align-items: center;
    flex: 0 0 auto;
    justify-content: flex-end;
    width: 100px; }

.navbar ul.nav {
  display: flex;
  flex-wrap: nowrap;
  align-items: center;
  justify-content: flex-end;
  list-style: none; }
  .navbar ul.nav li {
    cursor: pointer;
    transition: box-shadow 0.5s;
    /*下划线*/
    /*内容图标*/
    /*状态切换*/
    /* **prevent bootstrap*/ }
    .navbar ul.nav li:after {
      content: "";
      position: absolute;
      width: 100%;
      padding: 2px;
      background-color: #31ccff;
      transition: transform 0.5s;
      transform: scale(0, 1); }
    .navbar ul.nav li a,
    .navbar ul.nav li i {
      display: block;
      font-size: 2.4rem;
      font-style: normal;
      text-decoration: none; }
    .navbar ul.nav li.active {
      pointer-events: none; }
    .navbar ul.nav li.active a, .navbar ul.nav li:active a {
      color: #31ccff; }
    .navbar ul.nav li.active:after, .navbar ul.nav li:active:after, .navbar ul.nav li:hover:after {
      transform: scale(1, 1); }
    .navbar ul.nav li:hover:not(.active) {
      box-shadow: 0 3px 3px 0 rgba(0, 0, 0, 0.14), 0 1px 7px 0 rgba(0, 0, 0, 0.12), 0 3px 1px -1px rgba(0, 0, 0, 0.2); }
      .navbar ul.nav li:hover:not(.active) a {
        color: #fff;
        font-weight: 500; }
    .navbar ul.nav li a:hover,
    .navbar ul.nav li a:focus {
      background-color: transparent; }

.user-wrapper {
  margin: 0 -5px;
  /** 登录状态切换 */ }
  .user-wrapper > a {
    margin: 0 5px;
    cursor: pointer;
    font-size: 16px; }
    .user-wrapper > a[disabled] {
      pointer-events: none; }
    .user-wrapper > a:hover, .user-wrapper > a:focus {
      color: #fff; }
  .user-wrapper:not(.logged) .user, .user-wrapper:not(.logged) .logout, .user-wrapper.logged .login {
    pointer-events: none; }
  .user-wrapper .login:before {
    content: "";
    font-family: "iconfont" !important;
    font-style: normal;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale; }
  .user-wrapper .user:before {
    content: "";
    font-family: "iconfont" !important;
    font-style: normal;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale; }
  .user-wrapper .logout:before {
    content: "";
    font-family: "iconfont" !important;
    font-style: normal;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale; }

