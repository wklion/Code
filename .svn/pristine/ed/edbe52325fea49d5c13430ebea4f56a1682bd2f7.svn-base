/*底部按钮*/
#bottomMenu{
    position: absolute;
    height: 50px;
    width: 100%;
    bottom: 0;
    font-size: 15px;
    line-height: 50px;
    background-color: #222;
}
#bottomMenu .iconfont{
    position: absolute;
    right: 15px;
    width: 12px;
    font-size: 14px;
    text-align: center;
    cursor: pointer;
}
#bottomMenu .scenicCheck {
    margin: 10px 0 0 5px;
    position: relative;
    float: left;
    line-height: 30px;
    padding-left: 22px;
    color: #eee;
}
#bottomMenu .scenicCheck input[type=checkbox] {
    visibility: hidden;
}
#bottomMenu .scenicCheck label {
    cursor: pointer;
    position: absolute;
    width: 20px;
    height: 20px;
    top: 5px;
    left: 10px;
    background: #222;
    border:2px solid #0d3d88;
}
#bottomMenu .scenicCheck label:after {
    opacity: 0;
    content: '';
    position: absolute;
    width: 10px;
    height: 6px;
    background: transparent;
    top: 4px;
    left: 4px;
    border: 1px solid #fec04e;
    border-top: none;
    border-right: none;
    -webkit-transform: rotate(-45deg);
    -moz-transform: rotate(-45deg);
    -o-transform: rotate(-45deg);
    -ms-transform: rotate(-45deg);
    transform: rotate(-45deg);
}
#bottomMenu .scenicCheck label:hover::after {
    opacity: 0.5;
}
#bottomMenu .scenicCheck input[type=checkbox]:checked + label:after {
    opacity: 1;
}
